{
    "version": 2,
    "builds": [
      {
        "src": "requirements.txt",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "50mb"
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/socialsite/static/$1"
      },
      {
        "src": "/media/(.*)",
        "dest": "/media/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/config/wsgi.py"
      }
    ],
    "env": {
      "DJANGO_SETTINGS_MODULE": "config.settings.production",
      
      "SECRET_KEY": "@secret-key",
      "DEBUG": "false",
      "ALLOWED_HOSTS": "your-vercel-url.vercel.app"
    }
  }