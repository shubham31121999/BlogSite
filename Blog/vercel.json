{
    "builds": [
      {
        "src": "Blog/config/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "Blog/config/wsgi.py"
      }
    ],
    "env": {
      "DJANGO_SETTINGS_MODULE": "config.django.base",
      
      
      
    }
  }